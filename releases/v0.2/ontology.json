  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Replacing the dataset-specific pseudo-IDs with a common pseudo-ID across all of the project datasets that links together all datasets for a specific project."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Dataset Linkage"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreActivity"
  } ]
}, {
  "@id" : "https://w3id.org/shp#DatasetLinkageIdMapping",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "Linkage file maps the original identifiable ID (e.g. such as CHI) in the respective dataset to a project and dataset specific new UID. In other words, those UIDs are different for each dataset in a project. This is to prevent premature linkage of the data before is has been fully anonymised."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Dataset Linkage ID Mapping"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreProjectDocument"
  } ]
}, {
  "@id" : "https://w3id.org/shp#DatasetRelease",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "The project-specific data released to a researcher in the trusted research environment for researcher analysis. This consists of datasets (i.e., flat-files of the extracted, pseudonymised and linked data), Data Linkage Plan, Data Specification file, and summary data that are provided to a researcher in their project folder."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Dataset Release"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreActivity"
  } ]
}, {
  "@id" : "https://w3id.org/shp#DisclosureCheck",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "Aggregate results compiled by the researcher during their analysis of the released data in the Trusted Research Environment, are reviewed by TRE staff to ensure there are no risks of identifying individuals in the released data."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Disclosure Check"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#DataCheck"
  } ]
}, {
  "@id" : "https://w3id.org/shp#DisclosureCheckReport",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@type" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "@value" : "Report of the results of the disclosure checks, which indicates if the work of the Lead analyst has passed or failed the disclosure check."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Disclosure Check Report"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreReport"
  } ]
}, {
  "@id" : "https://w3id.org/shp#EntityCharacteristic",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A utility concept for referencing specific properties of an Entity. For example, if an entitiy represents a dataset, an EntityChracteristic can capture a collection of summary statistics such as min max values, etc."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Entitiy Characteristic"
  } ]
}, {
  "@id" : "https://w3id.org/shp#FlatFileCreation",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Datasets and Linkage dataset exported from SQL to flat-files"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "FlatFileCreation"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreActivity"
  } ]
}, {
  "@id" : "https://w3id.org/shp#IdLinkage",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Creating a unique identifier (UID) mapping table for each pseudonymised dataset, which maps the original identifier (non-pseudonymised) of the dataset to the new pseudonymised ID for that dataset."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ID Linkage"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreActivity"
  } ]
}, {
  "@id" : "https://w3id.org/shp#LeadAnalyst",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Lead Analyst is responsible for the whole cohort creation, data extraction, pseudonymisation and data linkage process of the project. Depending on the data safe haven, the analysts may or may not have the right to see and work on the non-anonymized data."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Lead Analyst"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.w3.org/ns/prov#Person"
  } ]
}, {
  "@id" : "https://w3id.org/shp#OriginalIdRemoval",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Once all datasets for the project have had pseudo-IDs applied and a linkage dataset created, the original IDs are removed from the UID mapping table (a dataset moves from Pre-Release to Release)"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Original ID Removal"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreActivity"
  } ]
}, {
  "@id" : "https://w3id.org/shp#ProjectIdAssignment",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Project IDs are assigned to a project by the Research Coordinator."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Project ID Assignment"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreActivity"
  } ]
}, {
  "@id" : "https://w3id.org/shp#Pseudonymization",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Techniques that replace, remove or transform information that identifies an individual (e.g. replacing date of birth with age, replacing postcode with a regional classification or demographic marker, replacing NHS number/CHI number with a pseudo-ID number)."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Pseudonymization"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/shp#TreActivity"
  } ]
}, {
  "@id" : "https://w3id.org/shp#RequestedVariables",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A collection of variables requested by the researcher and included in the Data Specification File."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Requested Variables"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.w3.org/ns/prov#Collection"
  } ]
}, {
  "@id" : "https://w3id.org/shp#ResearchCoordinator",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "In the Grampian Data Safe Haven, Research Coordinators facilitate the use of routinely collected health and population data for research. Research Coordinators also ensure all information governance and information security measures related to research projects, including reviewing project permissions, approved researcher access periods, appropriate training has been undertaken before and during access to the TRE. Research Coordinators support researchers from academic, NHS and industry to design, plan, gather permissions and conduct research involving health and administrative data."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Research Coordinator"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.w3.org/ns/prov#Person"
  } ]
}, {
  "@id" : "https://w3id.org/shp#Researcher",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A person who carries out academic, scientific or clinical research. Researchers who require a TRE (or SDE/Safe Haven), must have any research project approved by ethical permissions to undertake the research and require enhanced information governance and security training to become approved to access highly-sensitive data within the TRE."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Researcher"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.w3.org/ns/prov#Person"
  } ]
}, {
  "@id" : "https://w3id.org/shp#SelectedVariables",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A collection of Variables that were selected during the extraction process and are contained within a particular dataset."
  } ],
